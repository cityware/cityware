{
    "name": "cityware/cityware",
    "description": "Cityware",
    "version": "1.0.0",
    "type": "library",
    "keywords": [
        "framework",
        "library",
        "Cityware"
    ],
    "homepage": "http://www.cityware.com.br/",
    "license": "GPL-3.0",
    "autoload": {
        "psr-4": {
            "Cityware\\": "/"
        },
        "psr-0": {
            "Cityware\\": "/"
        }
    },
    "require": {
        "php": ">=5.5",
        "zendframework/zendframework": "~2.5",
        "neilime/zf2-twb-bundle": "2.*@stable",
        "cityware/city-chart": "dev-master",
        "cityware/city-datagrid": "dev-master",
        "cityware/city-db": "dev-master",
        "cityware/city-exception": "dev-master",
        "cityware/city-form": "dev-master",
        "cityware/city-format": "dev-master",
        "cityware/city-generator": "dev-master",
        "cityware/city-log": "dev-master",
        "cityware/city-mvc": "dev-master",
        "cityware/city-regex": "dev-master",
        "cityware/city-security": "dev-master",
        "cityware/city-services": "dev-master",
        "cityware/city-utility": "dev-master",
        "cityware/city-view": "dev-master"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "phpunit/PHPUnit": "~4.0"
    },
    "suggest": {
        "blanchonvincent/simple-memory-shared": "ZF2 module v2.1.0 - Provide a manager to share simply memory with storage options : file, session, segment, memcached, apc, zendshm and zenddisk.",
        "blanchonvincent/parallel-jobs": "ZF2 module v1.5.0 - Fork Manager to manage parallel tasks."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev",
            "dev-develop": "1.1-dev"
        }
    }
}