        "cityware/city-db": "dev-master",
        "cityware/city-exception": "dev-master",
        "cityware/city-form": "dev-master",
        "cityware/city-format": "dev-master",
        "cityware/city-generator": "dev-master",
        "cityware/city-log": "dev-master",
        "cityware/city-monitoring": "dev-master",
        "cityware/city-mvc": "dev-master",
        "cityware/city-parallel-jobs": "dev-master",
        "cityware/city-regex": "dev-master",
        "cityware/city-security": "dev-master",
